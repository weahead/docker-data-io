#!/bin/sh

set -e

SRC="/data.tar.gz"

if [ -n "${DATA_TAG}" ]; then
  data-io-registry unpack "${SRC}"
elif [ -n "${DATA_S3_DST}" ]; then
  data-io-s3 unpack "${SRC}"
else
  echo
  echo "Nowhere to send the packed data?"
fi

echo
echo "Unpacking data"
tar -xzf /data.tar.gz -C /
echo "done."

echo
echo "Finished successfully!"
